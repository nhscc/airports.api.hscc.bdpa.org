{
  "name": "airlines.xunn.io",
  "description": "https://airlines.xunn.io",
  "repository": {
    "type": "git",
    "url": "https://git.xunn.io/open-source/web-mirrored/airlines.xunn.io"
  },
  "version": "0.0.1",
  "scripts": {
    "init": "npx gulp eject",
    "dev": "npx next -p 3002",
    "build": "npx next build",
    "start": "npx next start",
    "test": "npx jest",
    "install-types": "npx flow-typed install",
    "debug-remote": "node --inspect-brk='0.0.0.0:7777'",
    "debug-remote-gulp": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/gulp",
    "debug-remote-jest": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/jest --runInBand",
    "test-ib": "npx jest --runInBand",
    "test-nc": "npx jest --no-cache",
    "clean-types": "npx gulp cleanTypes",
    "generate": "BABEL_ENV=generator npx babel config --out-dir .",
    "regenerate": "npx gulp regenerate",
    "bump-major": "npx bump --major --tag --commit --lock --all",
    "bump-minor": "npx bump --minor --tag --commit --lock --all",
    "bump-patch": "npx bump --patch --tag --commit --lock --all",
    "list-tasks": "npm run && echo 'Compiling gulp task list...' && npx gulp --tasks",
    "preversion": "npm run test-ib",
    "version": "git add --all"
  },
  "author": "Xunnamius",
  "license": "ISC",
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.preprocess.js"
    },
    "moduleNameMapper": {
      "^universe/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "expectedEnvVariables": [
    "BUNDLE_ANALYZE",
    "NODE_ENV|BABEL_ENV|APP_ENV"
  ],
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-flow": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "confusing-browser-globals": "^1.0.9",
    "dotenv": "^8.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.1.6",
    "next-transpile-modules": "^2.3.1",
    "pg": "^7.15.1",
    "pg-promise": "^10.3.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-source-map-support": "^2.1.1",
    "chalk": "^3.0.0",
    "del": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "fancy-log": "^1.3.3",
    "flow-bin": "^0.114.0",
    "flow-typed": "^2.6.2",
    "gulp": "^4.0.2",
    "gulp-tap": "^2.0.0",
    "inquirer": "^7.0.1",
    "jest": "^24.9.0",
    "parse-gitignore": "^1.0.1",
    "replace-in-file": "^5.0.2",
    "shelljs": "^0.8.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.16",
    "version-bump-prompt": "^5.0.6",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
