{
  "name": "airlines.xunn.io",
  "description": "https://airlines.xunn.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/open-source/web-mirrored/airlines.xunn.io"
  },
  "version": "0.0.1",
  "scripts": {
    "dev": "sudo -u x-runner npx next -p 3005",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "install-types": "flow-typed install",
    "debug-remote": "node --inspect-brk='0.0.0.0:7777'",
    "debug-remote-gulp": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/gulp",
    "debug-remote-jest": "node --inspect-brk='0.0.0.0:7777' ./node_modules/.bin/jest --runInBand",
    "test-ib": "jest --runInBand",
    "test-nc": "jest --no-cache",
    "clean-types": "gulp cleanTypes",
    "generate": "BABEL_ENV=generator babel config --out-dir .",
    "regenerate": "gulp regenerate",
    "list-tasks": "npm run && echo 'Compiling gulp task list...' && gulp --tasks"
  },
  "author": "Xunnamius",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.preprocess.js"
    },
    "moduleNameMapper": {
      "^universe/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "expectedEnvVariables": [
    "BUNDLE_ANALYZE",
    "NODE_ENV|BABEL_ENV|APP_ENV"
  ],
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "confusing-browser-globals": "^1.0.9",
    "dotenv": "^8.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.4",
    "next-transpile-modules": "^3.2.0",
    "pg": "^8.0.2",
    "pg-promise": "^10.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-source-map-support": "^2.1.1",
    "chalk": "^4.0.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "fancy-log": "^1.3.3",
    "flow-bin": "^0.122.0",
    "flow-typed": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-tap": "^2.0.0",
    "inquirer": "^7.1.0",
    "jest": "^25.3.0",
    "parse-gitignore": "^1.0.1",
    "replace-in-file": "^5.0.2",
    "shelljs": "^0.8.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.16",
    "version-bump-prompt": "^6.0.3",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}
